<!-- 프레임 생성 : Login -->
	<FRAME item="Login">
		<FRAMELOOK>NullFrameLook</FRAMELOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>800</W>
			<H>600</H>
		</BOUNDS>
		<TITLEBAR>false</TITLEBAR>
		<RESIZABLE>false</RESIZABLE>
		<FOCUSABLE>false</FOCUSABLE>
		<VISIBLE>false</VISIBLE>
	</FRAME>
	
<!-- Picture : 배경 이미지 -->
	<PICTURE item="Login_BackgrdImg" parent="Login">
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>800</W>
			<H>600</H>
		</BOUNDS>
		<STRETCH/>
		<BITMAP></BITMAP>
	</PICTURE>

    <!-- LoginFrame -->
	<FRAME item="LoginFrame" parent="Login">
		<FRAMELOOK>NullFrameLook</FRAMELOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>800</W>
			<H>600</H>
		</BOUNDS>
		<TITLEBAR>false</TITLEBAR>
		<RESIZABLE>false</RESIZABLE>
		<FOCUSABLE>false</FOCUSABLE>
		<VISIBLE>false</VISIBLE>
	</FRAME>


	<!-- Login Frame Image-->
	<PICTURE item="LoginFrameImage" parent="LoginFrame">
		<BOUNDS>
			<X>230</X>
			<Y>197</Y>
			<W>350</W>
			<H>200</H>
		</BOUNDS>
		<STRETCH/>
		<BITMAP>loginframe.png</BITMAP>
	</PICTURE>	

    <!-- Label -->
	<ANIMATION item="Login_ConnectingMsg" parent="Login">
	<PLAYMODE>repetition</PLAYMODE>
	<ANIMTEMPLATE>ConnectingEfecto</ANIMTEMPLATE>
		<BOUNDS>
			<X>380</X>
			<Y>270</Y>
			<W>50</W>
			<H>50</H>
		</BOUNDS>
		<STRETCH/>
		<VISIBLE>true</VISIBLE>
	</ANIMATION>	
 
	<!-- ServerWiew -->
	<SERVERVIEW item="SelectedServer" parent="LoginFrame">
		<BOUNDS>
			<X>580</X>
			<Y>411</Y>
			<W>540</W>
			<H>160</H>
		</BOUNDS>
	</SERVERVIEW>
	
	<!-- Label : ID -->
	<EDIT item="LoginID" parent="LoginFrame">
		<EDITLOOK>NullEditLook</EDITLOOK>
		<BOUNDS>
			<X>241</X>
			<Y>257</Y>
			<W>324</W>
			<H>26</H>
		</BOUNDS>
		<TEXT></TEXT>
		<VISIBLE>true</VISIBLE>
		<MAXLENGTH>22</MAXLENGTH>
	</EDIT>

	<!-- Label : Pass -->
	<EDIT item="LoginPassword" parent="LoginFrame">
		<EDITLOOK>NullEditLook</EDITLOOK>
		<PASSWORD>true</PASSWORD>
		<BOUNDS>
			<X>241</X>
			<Y>308</Y>
			<W>324</W>
			<H>26</H>
		</BOUNDS>
		<TEXT></TEXT>
		<VISIBLE>true</VISIBLE>
		<MAXLENGTH>22</MAXLENGTH>
	</EDIT>

    <!-- Login Failed Message -->
	<LABEL item="LoginError" parent="LoginFrame">
		<TEXTCOLOR>
			<R>200</R>
			<G>0</G>
			<B>0</B>
		</TEXTCOLOR>
		<BOUNDS>
			<X>235</X>
			<Y>359</Y>
			<W>200</W>
			<H>32</H>
		</BOUNDS>
		<TEXT></TEXT>
	</LABEL>
	
    <!-- Exit Button -->
	<BMBUTTON item="Exit" parent="LoginFrame">
		<BOUNDS>
			<X>371</X>
			<Y>345</Y>
			<W>96</W>
			<H>42</H>
		</BOUNDS>
		<DEFAULTKEY>ESC</DEFAULTKEY>
		<STRETCH/>
		<BITMAP type="up">exitlg_off_kr.png</BITMAP>
		<BITMAP type="over">exitlg_on_kr.png</BITMAP>
		<BITMAP type="down">exitlg_on_kr.png</BITMAP>
		<CONFIRMMESSAGE>STR:UI_LOGIN_05</CONFIRMMESSAGE> <!-- Do you want to quit the game? -->
		<CONFIRMLOOK>DefaultFrameLook</CONFIRMLOOK>
		<FOCUSABLE>FALSE</FOCUSABLE>
	</BMBUTTON>	
	
    <!-- Login Button -->	
	<BMBUTTON item="LoginOK" parent="LoginFrame">
		<BOUNDS>
			<X>474</X>
			<Y>345</Y>
			<W>96</W>
			<H>42</H>
		</BOUNDS>
		<DEFAULTKEY>ENTER</DEFAULTKEY>
		<STRETCH/>
		<BITMAP type="up">enterlg_off_kr.png</BITMAP>
		<BITMAP type="over">enterlg_on_kr.png</BITMAP>
		<BITMAP type="down">enterlg_on_kr.png</BITMAP>
	</BMBUTTON>
	
	<BUTTON item="RememberMe" parent="LoginFrame">
		<BUTTONLOOK>Custom1ButtonLook</BUTTONLOOK>
		<PUSHBUTTON/>
		<BOUNDS>
			<X>230</X>
			<Y>355</Y>
			<W>24</W>
			<H>24</H>
		</BOUNDS>
	</BUTTON>

